{
  "name": "vuejs.london",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build --modern=server",
    "dev": "nuxt",
    "start": "nuxt build --modern",
    "generate": "nuxt generate --modern=client",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint:changed": "yarn lint:eslint:changed && yarn lint:stylelint:changed && yarn lint:prettier:changed -c",
    "lint:changed:fix": "yarn lint:eslint:changed --fix && yarn lint:stylelint:changed --fix && yarn lint:prettier:changed --write",
    "lint:changed:compat": "LINT_BASE=compat yarn lint:eslint:changed && LINT_BASE=compat yarn lint:stylelint:changed",
    "lint:compat:all": "LINT_BASE=compat yarn lint:eslint:all && LINT_BASE=compat yarn lint:stylelint:all",
    "lint:eslint": "eslint --ignore-path .lintignore --ext .json,.js,.jsx,.ts,.tsx,.vue -c .eslintrc.js",
    "lint:eslint:changed": "./script/exec-changed.js json,js,jsx,ts,tsx,vue yarn lint:eslint",
    "lint:eslint:all": "yarn lint:eslint .",
    "lint:eslint:rule": "LINT_BASE=base yarn lint:eslint --rule",
    "lint:stylelint": "stylelint --ignore-path .lintignore --config .stylelintrc.js",
    "lint:stylelint:changed": "./script/exec-changed.js vue,css yarn lint:stylelint",
    "lint:stylelint:all": "yarn lint:stylelint 'src/**/*.@(css|vue)'",
    "lint:prettier": "prettier --ignore-path .lintignore --config prettier.config.js",
    "lint:prettier:changed": "./script/exec-changed.js json,js,jsx,ts,tsx,css,vue yarn lint:prettier",
    "lint:prettier:all": "yarn lint:prettier '**/*.@(css|vue|tsx?|jsx?|json)'"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@nuxtjs/onesignal": "3.0.0-beta.16",
    "@nuxtjs/pwa": "3.0.2",
    "gsap": "3.6.0",
    "normalize.css": "8.0.1",
    "nuxt": "2.14.6",
    "vue-easy-lightbox": "0.14.0"
  },
  "devDependencies": {
    "@nuxt/babel-preset-app": "2.14.6",
    "babel-eslint": "10.1.0",
    "browserslist-config-google": "2.0.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "0.5.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "22.0.0",
    "eslint-plugin-vue": "6.2.2",
    "eslint-plugin-vue-a11y": "0.0.31",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "open": "7.2.1",
    "postcss-calc": "7.0.4",
    "postcss-extend": "1.0.5",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.2.1",
    "postcss-nested-ancestors": "2.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-pxtorem": "5.1.1",
    "prettier": "2.1.2",
    "reset-css": "5.0.1",
    "stylelint": "13.7.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-no-unsupported-browser-features": "4.0.0"
  }
}
